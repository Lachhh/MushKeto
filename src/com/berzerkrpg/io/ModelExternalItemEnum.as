		static public function create(id:String, gs:int, kong:String, armorGames:String, pio:String, googlePlay:String, ios:String, amazon:String):ModelExternalItem {
			var m:ModelExternalItem = new ModelExternalItem(id, gs, kong, armorGames, pio, googlePlay, ios, amazon);
			if(!getFromId(id).isNull) throw new Error("Duplicate ID!");
			ALL.push(m);
			return m;
		}
		
		static public function createDLC(id:String, gs:int, kong:String, armorGames:String, pio:String, googlePlay:String, ios:String, amazon:String, idSteamDLC:uint):ModelExternalItem {
			var result:ModelExternalItem = create(id, gs, kong, armorGames, pio, googlePlay, ios, amazon);
			result.isDLC = true;
			result.idSteamDLC = idSteamDLC;
			return result;
		}
		
		static public function getFromId(id:String):ModelExternalItem {
			for (var i : int = 0; i < ALL.length; i++) {
				var g:ModelExternalItem = ALL[i] as ModelExternalItem;
				if(id == g.id) return g;
			}
			return NULL;
		}
		
		static public function getFromGoogleId(id:String):ModelExternalItem {
			for (var i : int = 0; i < ALL.length; i++) {
				var g:ModelExternalItem = ALL[i] as ModelExternalItem;
				if(id == g.idGoogle) return g;
			}
			return NULL;
		}
		
		static public function getFromIndex(index:int):ModelExternalItem {
			if(index >= ALL.length) return NULL;
			if(index < 0) return NULL;
			return ALL[index] as ModelExternalItem;
		}  
				
		static public function getNum():int {
			return ALL.length;
		}
